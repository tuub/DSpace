<?xml version="1.0" encoding="UTF-8"?>

<!-- DC.TITLE.TRANSLATED -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>title</dc-element>
    <dc-qualifier>translated</dc-qualifier>
    <repeatable>true</repeatable>
    <label>Translated title</label>
    <input-type>onebox</input-type>
    <language value-pairs-name="common_iso_languages">true</language>
    <hint>Enter German translation if your thesis is written in English et vice versa.</hint>
    <required>You have to enter a translated title.</required>
    <visibility></visibility>
    <type-bind>Doctoral Thesis</type-bind>
</field>

<!-- DC.TITLE.TRANSLATEDSUBTITLE -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>title</dc-element>
    <dc-qualifier>translatedsubtitle</dc-qualifier>
    <repeatable>true</repeatable>
    <label>Translated subtitle</label>
    <input-type>onebox</input-type>
    <language value-pairs-name="common_iso_languages">true</language>
    <hint>Enter German translation of your subtitle if your thesis is written in English et vice versa.</hint>
    <required></required>
    <visibility>workflow</visibility>
    <type-bind>Doctoral Thesis</type-bind>
</field>

