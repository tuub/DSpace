<!-- DC.DESCRIPTION.ABSTRACT -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>description</dc-element>
    <dc-qualifier>abstract</dc-qualifier>
    <repeatable>true</repeatable>
    <label>Abstract</label>
    <input-type>textarea</input-type>
    <language value-pairs-name="common_iso_languages">true</language>
    <hint>Enter a German and an English abstract. Please use "Add more" to enter abstracts in other languages.</hint>
    <required>You have to enter an abstract.</required>
    <type-bind>Preprint</type-bind>
</field>

<!-- DC.IDENTIFIER -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>identifier</dc-element>
    <dc-qualifier></dc-qualifier>
    <repeatable>true</repeatable>
    <label>Identifier(s)</label>
    <input-type value-pairs-name="common_identifiers">qualdrop_value</input-type>
    <hint>If the preprint has any identification numbers or codes associated with it, please enter the types and the actual numbers or codes below.</hint>
    <required></required>
    <type-bind>Preprint</type-bind>
</field>

<!-- TUB.SERIES.NAME -->
<field>
    <dc-schema>tub</dc-schema>
    <dc-element>series</dc-element>
    <dc-qualifier>name</dc-qualifier>
    <repeatable>false</repeatable>
    <label>Name of the series</label>
    <input-type>onebox</input-type>
    <hint>Enter the name of the series.</hint>
    <required></required>
    <type-bind>Preprint</type-bind>
</field>

<!-- TUB.SERIES.ISSUENUMBER -->
<field>
    <dc-schema>tub</dc-schema>
    <dc-element>series</dc-element>
    <dc-qualifier>issuenumber</dc-qualifier>
    <repeatable>false</repeatable>
    <label>Issue of the series</label>
    <input-type>onebox</input-type>
    <hint>Enter the issue number of the series.</hint>
    <required></required>
    <type-bind>Preprint</type-bind>
    <visibility>workflow</visibility>
</field>

<!-- DC.DESCRIPTION.SPONSORSHIP-->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>description</dc-element>
    <dc-qualifier>sponsorship</dc-qualifier>
    <repeatable>true</repeatable>
    <label>Sponsor or funder information</label>
    <input-type>onebox</input-type>
    <hint>Enter the name of the sponsor or funder, and/or project/grant name or ID. Please use the following syntax if applicable: EC/FP7/12345/EU/ProjectName/ProjectAcronym. Please separate multiple entries by semicolon.</hint>
    <required></required>
    <type-bind>Preprint</type-bind>
</field>

<!-- DC.TYPE.VERSION -->
<field>
    <dc-schema>dc</dc-schema>
    <dc-element>type</dc-element>
    <dc-qualifier>version</dc-qualifier>
    <repeatable>false</repeatable>
    <label>Version</label>
    <input-type value-pairs-name="type_versions">dropdown</input-type>
    <hint>Choose the type of version. For type "Preprint" choose either "Draft" for early versions or "Submitted Version" for a preprint that has been submitted for publication in a journal or book.</hint>
    <required>You have to choose the type of the published version.</required>
    <visibility>workflow</visibility>
	<type-bind>Preprint</type-bind>
</field>

<!-- TUB.PUBLISHER.UNIVERSITYORINSTITUTION -->
<field>
    <dc-schema>tub</dc-schema>
    <dc-element>publisher</dc-element>
    <dc-qualifier>universityorinstitution</dc-qualifier>
    <repeatable>false</repeatable>
    <label>Name of the publishing institution</label>
    <input-type value-pairs-name="publishing_institution">dropdown</input-type>
    <hint>The publishing institution is either  "Technische Universität Berlin" or "Universitätsverlag der TU Berlin".</hint>
    <required>You have to choose whether TU Berlin or Universitätsverlag is publishing this item.</required>
    <visibility>workflow</visibility>
    <type-bind>Preprint</type-bind>
</field>

<!-- TUB.ACCESSRIGHTS.DNB -->
<field>
    <dc-schema>tub</dc-schema>
    <dc-element>accessrights</dc-element>
    <dc-qualifier>dnb</dc-qualifier>
    <repeatable>false</repeatable>
    <label>Access rights DNB</label>
    <input-type value-pairs-name="accessrights_dnb">dropdown</input-type>
    <hint>Choose which rights the German National Library (DNB) has to display archived items.</hint>
    <required></required>
    <visibility>workflow</visibility>
    <type-bind>Preprint</type-bind>
</field>