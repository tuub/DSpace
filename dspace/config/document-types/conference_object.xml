    <input-type>onebox</input-type>
    <language value-pairs-name="common_iso_languages">true</language>
    <hint>Enter German translation of your subtitle if your conference contribution is written in English et vice versa.</hint>
    <required></required>
    <visibility>workflow</visibility>
    <type-bind>Conference Object</type-bind>
</field>

